{
    $
        var md = {
            name: "docx",
            description: "Docx document"
        }
    
    base-plugin( &md )

    [ globals
        $*
        {
            name "___"
            value "___"
            type "string"
        *$

    [ BuildVars
        {
            name "useDocx"
            value "true"
            type "string"

    [ Schemas
        {
            name "${md.name}"
            plugin_type "syntax_structure"
            hasWizzifier false
            { wizzifier
            [ includes
            [ transformers
            [ artifacts
                {
                    name "document"
                    outmime "${md.name}"
                    isDefault true
                    contentType "application/vnd.openxmlformats-officedocument.wordprocessingml.document"
            [ fileExtensions
                "${md.name}"