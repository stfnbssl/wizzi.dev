{
    $
        var md = {
            name: "philos",
            description: "Philos document"
        }
    
    base-plugin( &md )

    [ globals
        $*
        {
            name "___"
            value "___"
        *$

    [ BuildVars
        $*
        {
            name "___"
            value "true"
        *$

    [ Schemas
        {
            name "${md.name}"
            plugin_type "data"
            hasWizzifier false
            { wizzifier
            [ includes
            [ transformers
                {
                    name "extended"
                    outmime "json"
            [ artifacts
                {
                    name "document"
                    outmime "json"
                    isDefault true
                    contentType "appication/json"
            [ fileExtensions
                "${md.name}"