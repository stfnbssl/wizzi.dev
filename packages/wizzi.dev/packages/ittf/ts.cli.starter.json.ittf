{

    $
        var md = {
            name: "ts.cli.starter",
            title: "Typescript CLI starter App"
        }

    base-package-v2( &md )

    { App
        name "${md.name}"
        title "${md.title}"
    
    params/use( tsCommons
    params/use( tsCLI
        params/use( commander
            title "${md.title}"
            [ commands
                {
                    name "mp1"
                    label "Execute a meta provides get - 1"
                    function "metaProvides1"
                {
                    name "mp2"
                    label "Execute a meta provides get - 2"
                    function "metaProvides2"
        params/use( figlet
            title "${md.title}"
        params/use( mongoDb
    $$ params/use( tsExpress
    $$ params/use( tsDbMongoDb
    params/use( wizziDevPacki
    params/use( wizziDevHubProductions
    params/use( wizziDevHubDbCli

    [ BuildVars
        $*
        {
            name "___"
            value "___"
            type "string"
        *$

    [ globals
        $*
        {
            name "___"
            value "___"
            type "string"
        *$