{
    $
        var md = {
            name: "wfjob",
            npmName: "@wizzi/plugin.wfjob",
            description: "Wizzi Job Definition"
        }
    
    base-plugin( &md )

    [ globals
        $*
        {
            name "___"
            value "___"
            type "string"
        *$

    [ BuildVars
        $*
        {
            name "___"
            value "___"
            type "string"
        *$

    [ Schemas
        {
            name "${md.name}"
            plugin_type "syntax_structure"
            [ includes
                {
                    Name "Js"
                    name "js"
                    artifactName "js/module"
                    [ options
                    [ items
                        {
                            name "js"
                            parent "node"
                            tag "::js"
                            root "module"
            [ transformers
            [ artifacts
                {
                    name "document"
                    outmime "${md.name}"
                    isDefault true
                    contentType "text/plain"             
            [ fileExtensions
                "${md.name}"       
            


