{
    $
        var md = {
            name: "demo",
            title: "Meta demo",
            prefix: "demo"
        }

    base-meta( &md )

    [ BuildVars
        $*
        {
            name "___"
            value "___"
            type "string"
        *$

    [ globals
        $*
        {
            name "___"
            value "___"
            type "string"
        *$

    [ Categories
        {
            name "demo"
        {
            name "wizzi-starter"

    [ Productions
        {
            name "${md.prefix}Js"
            [ categories
                {
                    name "demo"
                {
                    name "wizzi-starter"
            { hints
                [ folders
                    {
                        name "root"

                        schema "js"
        $*
        {
            name "${md.prefix}Html"
        {
            name "${md.prefix}Css"
        *$