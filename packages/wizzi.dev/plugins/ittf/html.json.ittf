{
    $
        var md = {
            name: "html",
            description: "Html document"
        }
    
    base-plugin( &md )

    [ globals
        $*
        {
            name "___"
            value "___"
        *$

    [ BuildVars
        $*
        {
            name "___"
            value "___"
        *$

    [ Schemas
        {
            name "${md.name}"
            plugin_type "syntax_structure"
            hasNamespaces false
            hasTextContents false
            hasWizzifier true
            multiLineCommentBegin "<!--"
            multiLineCommentEnd "-->"
            hasWizzifier true
            { wizzifier
                [ parserImportCode
                    "var ${md.name}_parser = require('./parser')"
                [ parserInvokeCode
                    " _ ${md.name}_parser.parse(tobeWizzified)"
                useFormatAstMethods false
            [ includes
                {
                    Name "Js"
                    name "js"
                    artifactName "js/module"
                    [ options
                    [ items
                        {
                            name "js"
                            parent "node"
                            tag "::script"
                            root "module"
                {
                    Name "Css"
                    name "css"
                    artifactName "css/document"
                    [ options
                    [ items
                        {
                            name "css"
                            parent "node"
                            tag "::style"
                            root "css"
                {
                    Name "Svg"
                    name "svg"
                    artifactName "svg/document"
                    [ options
                    [ items
                        {
                            name "svg"
                            parent "node"
                            tag "::svg"
                            root "svg"
                {
                    Name "Vtt"
                    name "vtt"
                    artifactName "vtt/document"
                    [ options
                    [ items
                        {
                            name "vtt"
                            parent "node"
                            tag "::vtt"
                            root "vtt"
            [ transformers
            [ artifacts
                {
                    name "document"
                    outmime "${md.name}"
                    contentType "text/${md.name}"
                    isDefault true
            [ fileExtensions
                "${md.name}"
